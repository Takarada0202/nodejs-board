<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
    </head>
    <body>
        <form action="/getdata" method="GET">
            <input type="submit" value="getData">
        </form>
        <form action="/addPost" method="GET">
            <input type="submit" name="addPost" id="addPost" value="addPost">
        </form>
        <br>
        <% if(locals.data){ %>
            <% for(let i=0;i<data.length;i++){ %>
                <%= data[i].idx %>,<%= data[i].name %>,<%= data[i].title_ %><br>
            <% } %>
        <% } %>
    </body>
</html>